{
  "name": "vue-flexible-table",
  "version": "0.3.7",
  "description": "A flexible Vue table component.",
  "author": "Stefan Peters",
  "repository": {
    "type": "git",
    "url": "https://github.com/stefandesu/vue-flexible-table.git"
  },
  "license": "MIT",
  "main": "src/FlexibleTable.vue",
  "module": "dist/vue-flexible-table.common.js",
  "unpkg": "dist/vue-flexible-table.umd.min.js",
  "browser": {
    "./sfc": "src/FlexibleTable.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-flexible-table src/FlexibleTable.vue",
    "build-app": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix ./src/*.{js,vue} ./src/**/*.{js,vue}",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix"
    ],
    "src/*.{js,vue}": [
      "eslint --fix"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^10.5.4",
    "vue-template-compiler": "^2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
